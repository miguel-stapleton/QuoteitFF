{
  "functions": {
    "api/quotes/[id].ts": {
      "methods": ["GET", "PUT", "DELETE"]
    },
    "api/quotes/index.ts": {
      "methods": ["GET", "POST"]
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
